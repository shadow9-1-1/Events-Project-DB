                <tr>
                    <th scope="col">#</th>
                    <th scope="col">Event Name</th>
                    <th scope="col">Description</th>
                    <th scope="col">Price</th>
                    <th scope="col">Ticket Booked</th>
                    <th scope="col">img</th>
                </tr>
            </thead>
            <tbody>
                @for (int i = 0; i < @Model.Table.Rows.Count; i++)
                {
                    <tr>

                        <th scope="row">@(i + 1)</th>
                        <td>@Model.Table.Rows[i][1]</td>
                        <td>@Model.Table.Rows[i][2]</td>
                        <td>@Model.Table.Rows[i][6]</td>
